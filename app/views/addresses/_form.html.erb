<%= form_for @address do |f| %>

  <%= render 'errors', object: @address %>

  <p>
    <%= f.label :street %>
    <%= f.text_field :street %>
  </p>

  <p>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </p>

  <p>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </p>

  <p>
    <%= f.label :country %>
    <%= f.text_field :country %>
  </p>

  <p>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
  </p>

  <%= f.submit %>

<% end %>
